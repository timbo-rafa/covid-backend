{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@data-layer": [
        "src/data-layer"
      ],
      "@continent-covid": [
        "src/continent-covid"
      ],
      "@country-covid": [
        "src/country-covid"
      ],
      "@owid-data-import": [
        "src/integrations/owid"
      ],
      "@utils": [
        "src/utils"
      ]
    }
  }
}